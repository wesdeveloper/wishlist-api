{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-typescript/base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0,
      "max-len": 0,
      "consistent-return": 0,
      "max-classes-per-file": 0
    }
}
