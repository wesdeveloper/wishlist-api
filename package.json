{
  "engines": {
    "node": "14.16.0"
  },
  "name": "wishlist-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "npm run build && node dist/bin/index.js",
    "start:dev": "ts-node-dev bin/index.ts",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "jest",
    "test:unit:dev": "jest --watchAll",
    "test:functional": "cross-env NODE_ENV=production && jest --projects ./test",
    "test:functional-dev": "jest --projects ./test --watchAll",
    "knex:migrate:make": "knex migrate:make",
    "knex:migrate:latest": "knex migrate:latest",
    "knex:migrate:rollback": "knex migrate:rollback",
    "docker:dev": "docker-compose up -d",
    "docker:dev:logs": "docker-compose logs -f wishlistapi",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesdeveloper/wishlist-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wesdeveloper/wishlist-api/issues"
  },
  "homepage": "https://github.com/wesdeveloper/wishlist-api#readme",
  "devDependencies": {
    "@types/chance": "^1.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chance": "^1.1.7",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "winston": "^3.3.3"
  }
}
